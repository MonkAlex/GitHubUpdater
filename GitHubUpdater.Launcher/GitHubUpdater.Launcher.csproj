    <PostBuildEvent>
      md "$(TargetDir)\@(VersionNumber)"
      xcopy /y /r /e "$(ProjectDir)\..\GitHubUpdater.WPF\bin\$(ConfigurationName)" "$(TargetDir)\@(VersionNumber)"
